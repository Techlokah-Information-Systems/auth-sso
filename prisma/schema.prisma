generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id String @id @default(cuid())
  clerk_id String @unique
  email String @unique
  first_name String?
  last_name String?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}